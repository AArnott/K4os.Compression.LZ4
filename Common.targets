<Project>
  <PropertyGroup>
    <Version>1.1.11</Version>
    <AssemblyVersion>1.1.11</AssemblyVersion>
    <FileVersion>1.1.11</FileVersion>
  </PropertyGroup>
  <PropertyGroup>
    <Product>K4os.Compression.LZ4</Product>
    <Authors>Milosz Krajewski</Authors>
    <Description>Port of LZ4 compression algorithm for .NET</Description>
    <PackageLicenseUrl>https://raw.githubusercontent.com/MiloszKrajewski/K4os.Compression.LZ4/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/MiloszKrajewski/K4os.Compression.LZ4</PackageProjectUrl>
    <RepositoryUrl>https://github.com/MiloszKrajewski/K4os.Compression.LZ4</RepositoryUrl>
    <PackageIconUrl>https://github.com/MiloszKrajewski/K4os.Compression.LZ4/blob/master/doc/icon.png?raw=true</PackageIconUrl>
  </PropertyGroup>
  <!-- Signed in release only -->
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>../../K4os.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <!-- Internals visible to test when not release -->
  <ItemGroup Condition=" '$(Configuration)' != 'Release' ">
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleToAttribute">
      <_Parameter1>$(AssemblyName).Test</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>
  <!-- Optimize when... optimized -->
  <PropertyGroup Condition=" '$(Configuration)' == 'Optimized' ">
    <Optimize>true</Optimize>
  </PropertyGroup>
</Project>